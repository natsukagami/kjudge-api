{
  "name": "kjudge-api",
  "version": "0.0.2-0",
  "description": "A pluggable API for automated judging systems.",
  "main": "build/models/models.js",
  "scripts": {
    "postinstall": "cd src && typings install && cd ../ && npm run build",
    "prebuild": "rimraf build",
    "build": "(tsc -p src/ || true) && gulp",
    "test": "mocha build/test --require source-map-support/register || true",
    "clean": "rimraf build",
    "start": "node build/app/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natsukagami/kjudge-api.git"
  },
  "keywords": [
    "online-judge"
  ],
  "author": "Natsu Kagami",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/natsukagami/kjudge-api/issues"
  },
  "homepage": "https://github.com/natsukagami/kjudge-api#readme",
  "devDependencies": {},
  "dependencies": {
    "gulp": "^3.9.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "jsonminify": "^0.4.1",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta5",
    "shell-escape": "^0.2.0",
    "source-map-support": "^0.4.2",
    "temp": "^0.8.3"
  }
}
