  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "gulp": "^3.9.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
